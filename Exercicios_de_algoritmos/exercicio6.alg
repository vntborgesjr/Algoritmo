algoritmo "semnome"
// Função :
// Autor :
// Data : 15/08/2020
// Seção de Declarações 
var
 ant, n, suc: inteiro
inicio
// Seção de Comandos
 Escreva("Digite um número: ")
 Leia(n)
 ant <- n - 1
 suc <- n + 1
 Escreval("O antecessor de", n, " é", ant)
 Escreva("O sucessor de", n, " é", suc)
fimalgoritmo
